{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/pages/*"],
      "@common/*": ["src/components/common/*"],
      "@customer-center/*": ["src/components/customer-center/*"],
      "@main/*": ["src/components/main/*"],
      "@trainer-detail/*": ["src/components/trainer-detail/*"],
      "@trainer-list/*": ["src/components/trainer-list/*"],
      "@user/*": ["src/components/user/*"],
      "@user-trainer/*": ["src/components/user-trainer/*"]
    },
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext", "ES6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
